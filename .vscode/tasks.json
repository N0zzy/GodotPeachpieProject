{
       "version": "2.0.0",
       "tasks": [
              {
                     "label": "player",
                     "command": "../Godot_v4.1-beta3_mono_win64_console.exe",
                     "type": "process",
                     "args": [
                            "--path",
                            "${workspaceRoot}/GDApplication",
                     ],
                     "problemMatcher": "$msCompile"
              },
              {
                     "label": "editor",
                     "command": "../Godot_v4.1-beta3_mono_win64_console.exe",
                     "type": "process",
                     "args": [
                            "--path",
                            "${workspaceRoot}/GDApplication",
                            "--editor"
                     ],
                     "problemMatcher": "$msCompile"
              },
              {
                     "label": "sdkbuild",
                     "command": "dotnet",
                     "type": "process",
                     "args": [
                            "build",
                            "--project",
                            "${workspaceFolder}/Utils/SharpieSdk/SharpieSdk.csproj"
                     ],
                     "problemMatcher": "$msCompile"
              },
              {
                     "label": "sdkrun",
                     "command": "dotnet",
                     "type": "process",
                     "args": [
                            "run",
                            "--project",
                            "${workspaceFolder}/Utils/SharpieSdk/SharpieSdk.csproj"
                     ],
                     "problemMatcher": "$msCompile"
              },
              {
                     "label": "compilecsbuild",
                     "command": "dotnet",
                     "type": "process",
                     "args": [
                            "build",
                            "--project",
                            "${workspaceFolder}/Utils/GodotCompile/GodotCompile.csproj"
                     ],
                     "problemMatcher": "$msCompile"
              },
              {
                     "label": "compilecsrun",
                     "command": "dotnet",
                     "type": "process",
                     "args": [
                            "run",
                            "--project",
                            "${workspaceFolder}/Utils/GodotCompile/GodotCompile.csproj"
                     ],
                     "problemMatcher": "$msCompile"
              }
       ]
}